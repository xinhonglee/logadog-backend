    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.4.1",
    "dotenv-safe": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "generate-password": "^1.5.0",
    "geolib": "^3.2.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.4",
    "mongoose": "^5.7.1",
    "mongoose-aggregate-paginate-v2": "^1.0.4",
    "mongoose-paginate-v2": "^1.3.1",
