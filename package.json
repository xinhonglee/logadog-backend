    "ejs": "^2.7.4",
    "email-templates": "^7.0.2",
    "eslint": "^6.5.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.7",
    "mocha": "^6.2.1",
